    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.0",
    "crypto-js": "^4.1.1",
    "isbot": "^3.6.8",
    "lucide-react": "^0.308.0",
    "luxon": "^3.4.3",
    "nodemailer": "^6.9.9",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.0",
    "ramda": "^0.29.0",
    "ramda-adjunct": "^4.1.1",
