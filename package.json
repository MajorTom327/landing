    "@radix-ui/react-toggle": "^1.0.3",
    "@react-email/components": "^0.0.12",
    "@remix-run/css-bundle": "2.8.1",
    "@remix-run/node": "2.4.1",
    "@remix-run/react": "2.3.1",
